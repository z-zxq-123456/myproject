<?xml version="1.0" encoding="UTF-8"?>
<fileConvert name="txtFormatExample" type="txt" encoding="UTF-8" br="\r">

    <head sepType="F" ident="H" >
        <field name="SYSTEM_ID" length="8" trim="Y"/>
        <field name="ROWCOUNT" length="8" trim="Y" type="int" lr="L" pd="0"/>
        <field name="RUNDATE" length="8" trim="Y"/>
    </head>

    <body sepType="S" separator="@|$" ident="B">
        <field name="BASE_ACCT_NO" trim="Y"/>
        <field name="ACCT_TYPE" trim="Y"/>
        <field name="CCY" trim="Y"/>
        <field name="AMOUNT"  type="double" parseBeanId="amountParse"/>
    </body>

    <tail sepType="F" ident="T">
        <field name="TOTAL_AMOUNT" length="25" type="double" lr="L" pd="0"/>
    </tail>
</fileConvert>
