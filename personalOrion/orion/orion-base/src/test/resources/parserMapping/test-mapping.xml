<!--<!DOCTYPE mappings SYSTEM "http://www.dcits.com/xml/dtd/parserMapping.dtd">-->
<mappings useMapping="global">
    <mapping id="Financial-9999-0101">
        <in ref="[in]">
            <map name="BODY" ref="[in][BODY]" remove="SERV_DETAIL,SERV_DETAIL1">
                <list name="SERR" alias="SERV_DETAIL" source="[in][BODY][SERV_DETAIL]" ref="#SERV_DETAIL" index="index" remove="AMOUNT">
                    <item name="SEO_NO">#index+1</item>
                    <item name="BASE_ACCT_NO">[in][BODY][SERV_DETAIL1][#index][BASE_ACCT_NO]</item>
                </list>
                <if test="[in][BODY][SER] != null">
                    <item name="SEO_NO">'0'</item>
                </if>
                <if test="[in][BODY][SER] == null">
                    <item name="SEO_NO">'1'</item>
                </if>
            </map>
        </in>
    </mapping>
