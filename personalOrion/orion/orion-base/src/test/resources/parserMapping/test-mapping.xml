    <mapping id="Financial-9999-0106" useMapping="global">
        <in ref="[in]" useMapping="">
            <map name="BODY" ref="[in][BODY]" remove="ACCT_NO">
                <item name="BASE_ACCT_NO">[in][BODY][ACCT_NO]</item>
            </map>
        </in>
        <out ref="[out]" useMapping="">
            <map name="BODY" ref="[in][BODY]" remove="BASE_ACCT_NO">
                <item name="ACCT_NO">[in][BODY][BASE_ACCT_NO]</item>
            </map>
        </out>
    </mapping>

</mappings>