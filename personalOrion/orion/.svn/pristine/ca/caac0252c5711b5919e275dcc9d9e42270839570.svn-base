package com.dcits.orion.api.model;

import com.dcits.galaxy.base.bean.AbstractBean;
import com.dcits.galaxy.base.data.Request;

/**
 * 事件处理模型
 * <p/>
 * Created by Tim on 2015/11/17.
 */
public class EventRequest extends Request {

    private static final long serialVersionUID = -142732292270827495L;

    /**
     * 事件产品类型
     */
    private String eventProdType;

    /**
     * 产品类型
     */
    private String prodType;

    /**
     * 事件类型
     */
    private String eventType;

    /**
     * 事件服务ID
     */
    private String eventServiceId;

    private int eventIndex = 0;

    /**
     * 事件模型
     */
    private AbstractBean eventModel;

    public String getProdType() {
        return prodType;
    }

    public void setProdType(String prodType) {
        this.prodType = prodType;
    }

    public AbstractBean getEventModel() {
        return eventModel;
    }

    public void setEventModel(AbstractBean eventModel) {
        this.eventModel = eventModel;
    }

    public String getEventType() {
        return eventType;
    }

    public void setEventType(String eventType) {
        this.eventType = eventType;
    }

    public String getEventProdType() {
        return eventProdType;
    }

    public void setEventProdType(String eventProdType) {
        this.eventProdType = eventProdType;
    }

    public int getEventIndex() {
        return eventIndex;
    }

    public void setEventIndex(int eventIndex) {
        this.eventIndex = eventIndex;
    }

    public String getEventServiceId() {
        return eventServiceId;
    }

    public void setEventServiceId(String eventServiceId) {
        this.eventServiceId = eventServiceId;
    }
}
