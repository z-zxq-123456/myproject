<?xml version="1.0" encoding="UTF-8"?>
<fileConvert name="subFormatExample" type="txt" encoding="UTF-8">

    <head sepType="F" ident="SH" >
        <field name="HEAD" length="1" trim="Y"/>
        <field name="SYSTEM_ID" length="8" trim="Y"/>
        <field name="ROWCOUNT" length="8" trim="Y" type="int"/>
        <field name="RUNDATE" length="8" trim="Y"/>
    </head>

    <body sepType="S" separator="@|$" ident="SB" clazz="rb.eod.Tran">
        <field name="BODY" trim="Y"/>
        <field name="BASE_ACCT_NO" trim="Y"/>
        <field name="ACCT_TYPE" trim="Y"/>
        <field name="CCY" trim="Y"/>
        <field name="AMOUNT"  type="double" parseBeanId="amountParse"/>
    </body>

    <tail sepType="F" ident="ST">
        <field name="TAIL" length="1"/>
        <field name="TOTAL_AMOUNT" length="25" type="double" parseBeanId="amountParse"/>
    </tail>
</fileConvert>

