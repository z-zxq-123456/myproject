    where SER_MTD_ID = #{serMtdId,jdbcType=INTEGER}
  </update>
  <insert id="insert" parameterType="com.dcits.ensemble.om.oms.module.serv.EcmSerMtdinfo">
    insert into ECM_SER_MTDINFO
