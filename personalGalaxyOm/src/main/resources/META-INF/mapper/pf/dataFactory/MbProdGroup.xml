<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcits.ensemble.om.pf.dao.dataFactory.MbProdGroupDao">
  <!-- Created by admin on 2016/04/28 13:43:54. -->
  <select id="selectByPrimaryKey" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup">
    select *
    from mb_prod_group
    where PROD_TYPE = #{prodType,jdbcType=VARCHAR}
    <if test="prodSubType != null">
      AND PROD_SUB_TYPE = #{prodSubType,jdbcType=VARCHAR}
    </if>
  </select>
  <select id="getMaxSeqNo" parameterType="java.lang.String" resultType="java.lang.String">
    select MAX(SEQ_NO+0)
    from mb_prod_group
    where PROD_TYPE = #{prodType,jdbcType=VARCHAR}
  </select>
  <select id="selectByPrimaryKeyList" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup">
    select *
    from mb_prod_group
    where PROD_TYPE = #{prodType,jdbcType=VARCHAR}
  </select>
  <delete id="deleteByPrimaryKey" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup">
    delete from mb_prod_group
    where PROD_TYPE = #{prodType,jdbcType=VARCHAR}
      <if test="prodSubType != null">
        AND PROD_SUB_TYPE = #{prodSubType,jdbcType=VARCHAR}
      </if>
  </delete>
  <update id="updateByPrimaryKey" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup">
    update mb_prod_group
    <set>
      <if test="company != null">
        COMPANY = #{company,jdbcType=VARCHAR}
      </if>
    </set>
    where PROD_TYPE = #{prodType,jdbcType=VARCHAR}
        AND PROD_SUB_TYPE = #{prodSubType,jdbcType=VARCHAR}
  </update>
  <insert id="insert" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup">
    insert into mb_prod_group
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="prodType != null">
        PROD_TYPE,
      </if>
      <if test="prodSubType != null">
        PROD_SUB_TYPE,
      </if>
      <if test="seqNo != null">
        SEQ_NO,
      </if>
      <if test="defaultProd != null">
        DEFAULT_PROD,
      </if>
      <if test="company != null">
        COMPANY,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="prodType != null">
        #{prodType,jdbcType=VARCHAR},
      </if>
      <if test="prodSubType != null">
        #{prodSubType,jdbcType=VARCHAR},
      </if>
      <if test="seqNo != null">
        #{seqNo,jdbcType=VARCHAR},
      </if>
      <if test="defaultProd != null">
        #{defaultProd,jdbcType=VARCHAR},
      </if>
      <if test="company != null">
        #{company,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <select id="getSubProdByProdType" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbProdGroup" resultType="com.dcits.ensemble.om.pf.module.PkList">
    select
      PROD_SUB_TYPE pk_value, PROD_SUB_TYPE pk_desc
    FROM mb_prod_group
   where PROD_TYPE = #{prodType,jdbcType=VARCHAR}
  </select>
</mapper>