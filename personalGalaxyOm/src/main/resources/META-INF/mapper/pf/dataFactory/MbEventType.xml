<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dcits.ensemble.om.pf.dao.dataFactory.MbEventTypeDao">
  <!-- Created by zhangyjw on 2015/09/25 15:32:27. -->
  <select id="selectByPrimaryKey" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    where EVENT_TYPE = #{eventType,jdbcType=VARCHAR}
    AND COLUMN_STATUS !='W'
  </select>
  <select id="getAll" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    WHERE  COLUMN_STATUS !='W'
  </select>
  <select id="getIsStandardAllY" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    WHERE  COLUMN_STATUS !='W' and IS_STANDARD='Y'
  </select>
  <select id="getAllContrast" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    where COLUMN_STATUS!='Y'
  </select>
  <select id="getAllN" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    WHERE   IS_STANDARD = #{isStandard,jdbcType=VARCHAR}
  </select>
  <select id="getEventTypeByClass" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    where EVENT_CLASS = #{eventClass,jdbcType=VARCHAR}
    AND   event_type in (SELECT a.assemble_id FROM mb_prod_define a,mb_prod_type b WHERE a.prod_type = b.prod_type AND b.prod_range = 'B' AND a.assemble_type ='EVENT')
    AND COLUMN_STATUS !='W'
  </select>
  <delete id="deleteByPrimaryKey" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    delete from mb_event_type
    where EVENT_TYPE = #{eventType,jdbcType=VARCHAR}
  </delete>
  <update id="updateByPrimaryKey" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    update mb_event_type
    <set>
      <if test="eventDesc != null">
        EVENT_DESC = #{eventDesc,jdbcType=VARCHAR},
      </if>
      <if test="eventClass != null">
        EVENT_CLASS = #{eventClass,jdbcType=VARCHAR},
      </if>
      <if test="processMethod != null">
        PROCESS_METHOD = #{processMethod,jdbcType=VARCHAR},
      </if>
      <if test="isStandard != null">
        IS_STANDARD = #{isStandard,jdbcType=VARCHAR},
      </if>
      <if test="status != null">
        STATUS = #{status,jdbcType=VARCHAR},
      </if>
      <if test="company != null">
        COMPANY = #{company,jdbcType=VARCHAR}
      </if>
    </set>
    where EVENT_TYPE = #{eventType,jdbcType=VARCHAR}
  </update>
  <insert id="insert" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    insert into mb_event_type
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="eventType != null">
        EVENT_TYPE,
      </if>
      <if test="eventDesc != null">
        EVENT_DESC,
      </if>
      <if test="eventClass != null">
        EVENT_CLASS,
      </if>
      <if test="processMethod != null">
        PROCESS_METHOD,
      </if>
      <if test="isStandard != null">
        IS_STANDARD,
      </if>
      <if test="status != null">
        STATUS,
      </if>
      <if test="company != null">
        COMPANY,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="eventType != null">
        #{eventType,jdbcType=VARCHAR},
      </if>
      <if test="eventDesc != null">
        #{eventDesc,jdbcType=VARCHAR},
      </if>
      <if test="eventClass != null">
        #{eventClass,jdbcType=VARCHAR},
      </if>
      <if test="processMethod != null">
        #{processMethod,jdbcType=VARCHAR},
      </if>
      <if test="isStandard != null">
        #{isStandard,jdbcType=VARCHAR},
      </if>
      <if test="status != null">
        #{status,jdbcType=VARCHAR},
      </if>
      <if test="company != null">
       #{company,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
  <select id="getByEventClass" parameterType="java.lang.String" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    where EVENT_CLASS = #{eventClass,jdbcType=VARCHAR}
  </select>
  <select id="getForCheck" parameterType="java.lang.String" resultType="java.lang.String">
    select EVENT_CLASS
    from mb_event_type
    where EVENT_TYPE = #{eventType,jdbcType=VARCHAR}
  </select>
  <select id="getEventDesc" parameterType="java.lang.String" resultType="java.lang.String">
    select EVENT_DESC
    from mb_event_type
    where EVENT_TYPE = #{eventType,jdbcType=VARCHAR}
  </select>
  <select id="getByEventType" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
    select *
    from mb_event_type
    where EVENT_TYPE = #{eventType,jdbcType=VARCHAR}
  </select>
  <select id="getByBmodule" parameterType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType" resultType="com.dcits.ensemble.om.pf.module.dataFactory.MbEventType">
